# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /login               controllers.Assets.at(path="/public/html/", file="login.html")
POST	/login				controllers.Application.loginSubmit()
POST	/adduser				controllers.Application.addUser()
GET	/adduser				controllers.Assets.at(path="/public/html/", file="newstudent.html")
GET     /logout               controllers.Application.logout()
GET		/welcome			controllers.Assets.at(path="/public/html/", file="welcome.html")
GET		/map				controllers.Assets.at(path="/public/html/", file="map.html")
GET		/addingequationshome			controllers.Assets.at(path="/public/html/", file="addingequationshome.html")
