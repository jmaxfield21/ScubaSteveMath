# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /login               controllers.Assets.at(path="/public/html/", file="login.html")
POST	/login				controllers.Application.loginSubmit()
GET		/welcome			controllers.Assets.at(path="/public/html/", file="welcome.html")
GET		/addingequationshome			controllers.Assets.at(path="/public/html/", file="addingequationshome.html")